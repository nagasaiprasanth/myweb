pipeline{
    agent any
     tools {
        maven 'M2_HOME'
    }
    
    environment{
        PATH = "/opt/maven/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                url: 'https://github.com/nagasaiprasanth/myweb.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean sonar:sonar package"
                
            }
        }
        
    }
}
